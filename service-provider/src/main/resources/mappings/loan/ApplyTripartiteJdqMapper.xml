<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rongdu.loans.loan.dao.ApplyTripartiteJdqDao">
    
	<sql id="applyTripartiteJdqColumns">
		a.id AS "id",
		a.tripartite_no AS "tripartiteNo",
		a.apply_id AS "applyId",
		a.create_by AS "createBy",
		a.create_time AS "createTime",
		a.update_by AS "updateBy",
		a.update_time AS "updateTime",
		a.del AS "del",
		a.status AS "status"
	</sql>
	
	<sql id="applyTripartiteJdqJoins">
	</sql>
    
	<sql id="insertValues">
	(		
		#{id},
		#{tripartiteNo},
		#{applyId},
		#{createBy},
		#{createTime},
		#{updateBy},
		#{updateTime},
		#{del},
		#{status}
	)	
	</sql>
	
	<sql id="insertBatchValues">
	(		
		#{entity.id},
		#{entity.tripartiteNo},
		#{entity.applyId},
		#{entity.createBy},
		#{entity.createTime},
		#{entity.updateBy},
		#{entity.updateTime},
		#{entity.del},
		#{entity.status}
	)	
	</sql>
	
	<sql id="insertColumns">
	(		
		id,
		tripartite_no,
		apply_id,
		create_by,
		create_time,
		update_by,
		update_time,
		del,
		status
	) 
	</sql>
	
	<sql id="updateColumns">		
		tripartite_no = #{tripartiteNo},
		apply_id = #{applyId},
		update_by = #{updateBy},
		update_time = #{updateTime},
		status = #{status}
	</sql>
	
	<sql id="updateSelectiveColumns">
		<set>
			<if test="entity.tripartiteNo != null">
				tripartite_no = #{entity.tripartiteNo},
			</if>
			<if test="entity.applyId != null">
				apply_id = #{entity.applyId},
			</if>
			<if test="entity.updateBy != null">
				update_by = #{entity.updateBy},
			</if>
			<if test="entity.updateTime != null">
				update_time = #{entity.updateTime},
			</if>
			<if test="entity.status != null">
				status = #{entity.status},
			</if>
		</set>
	</sql>
	
	<sql id="whereByCriteriaList">
		<where>
			<foreach collection="criteriaList" item="criteria" separator=" AND ">
				<if test="criteria.valid">
					<trim prefix="(" suffix=")" prefixOverrides="AND |OR ">
						<foreach collection="criteria.pairList" item="pair">
							<choose>
								<when test="pair.criterion.singleValue">
									${pair.logic} ${pair.criterion.column}
									${pair.criterion.match}
									${pair.criterion.value}
								</when>
								<when test="pair.criterion.betweenValue">
									${pair.logic} ${pair.criterion.column}
									${pair.criterion.match}
									${pair.criterion.value} and
									${pair.criterion.secondValue}
								</when>
								<when test="pair.criterion.listValue">
									${pair.logic} ${pair.criterion.column} ${pair.criterion.match}
									<foreach collection="pair.criterion.value" item="item" open="(" close=")" separator=",">
										#{item}
									</foreach>
								</when>
							</choose>
						</foreach>
					</trim>
				</if>
			</foreach>
		</where>
	</sql>
	
	<sql id="whereByCriteria">
		<where>
				<if test="criteria.valid">
					<trim prefix="(" suffix=")" prefixOverrides="AND |OR ">
						<foreach collection="criteria.pairList" item="pair">
							<choose>
								<when test="pair.criterion.singleValue">
									${pair.logic} ${pair.criterion.column}
									${pair.criterion.match}
									${pair.criterion.value}
								</when>
								<when test="pair.criterion.betweenValue">
									${pair.logic} ${pair.criterion.column}
									${pair.criterion.match}
									${pair.criterion.value} and
									${pair.criterion.secondValue}
								</when>
								<when test="pair.criterion.listValue">
									${pair.logic} ${pair.criterion.column} ${pair.criterion.match}
									<foreach collection="pair.criterion.value" item="item" open="(" close=")" separator=",">
										#{item}
									</foreach>
								</when>
							</choose>
						</foreach>
					</trim>
				</if>
		</where>
	</sql>
	
   <sql id="orderBy">
		ORDER BY a.create_time DESC
	</sql>
	
   <sql id="pageOrderBy">
		<choose>
			<when test="page !=null and page.orderBy != null and page.orderBy != ''">
				ORDER BY ${page.orderBy}
			</when>
			<otherwise>
				ORDER BY a.create_time DESC
			</otherwise>
		</choose>
	</sql>
  
  	<select id="get" resultType="ApplyTripartiteJdq">
		SELECT 
		<include refid="applyTripartiteJdqColumns"/>
		FROM loan_apply_tripartite_jdq a
		<include refid="applyTripartiteJdqJoins"/>
		WHERE a.id = #{id}
	</select>

  	<select id="getById" resultType="ApplyTripartiteJdq">
		SELECT 
		<include refid="applyTripartiteJdqColumns"/>
		FROM loan_apply_tripartite_jdq a
		<include refid="applyTripartiteJdqJoins"/>
		WHERE a.id = #{id}
	</select>
		
    <select id="getByCriteria" resultType="ApplyTripartiteJdq">
