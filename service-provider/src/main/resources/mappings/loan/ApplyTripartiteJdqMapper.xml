	</update>

	<update id="updateByIdSelective">
		UPDATE loan_apply_tripartite_jdq		
		<include refid="updateSelectiveColumns"/>
		WHERE id = #{entity.id}
	</update>

	<update id="delete">
		UPDATE loan_apply_tripartite_jdq
		SET del = 1
		,update_by=#{updateBy}
